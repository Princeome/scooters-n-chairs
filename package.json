    "next": "^13.4.4",
    "next-sitemap": "^2.5.19",
    "pino": "^7.6.1",
    "pino-pretty": "^7.3.0",
    "postcss": "^8.4.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
